{
  "name": "djs-quick-starter",
  "version": "1.0.0",
  "description": "Djs quick starter is a quick starter for discord.js with commands, events handler and even more features",
  "main": "lib/index.js",
  "repository": "https://github.com/Expensiveee/djs-quick-starter",
  "author": "expensiveee <theexpensiveee@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^15.12.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@yarnpkg/pnpify": "^3.0.0-rc.6",
    "eslint": "^7.29.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --cache --fix",
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  }
}
